{
  "compilerOptions": {
    "rootDir": "./src",
    "baseUrl": "./src",
    "outDir": "./dist",
    "lib": [
      "ES5",
      "ES6",
      "DOM"
    ],
    "allowJs": true,
    "target": "ES5",
    "jsx": "react",
    "module": "CommonJS",
    "esModuleInterop": true,
    "strictNullChecks": true,
    "inlineSourceMap": true,
    "typeRoots": [
      "node_modules/@types"
    ]
  }
}